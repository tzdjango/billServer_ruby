<p id="notice"><%= notice %></p>

<h1>Listing Read Data</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @read_data.each do |read_datum| %>
      <tr>
        <td><%= read_datum.data %></td>
        <td><%= link_to 'Show', read_datum %></td>
        <td><%= link_to 'Edit', edit_read_datum_path(read_datum) %></td>
        <td><%= link_to 'Destroy', read_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Read datum', new_read_datum_path %>
